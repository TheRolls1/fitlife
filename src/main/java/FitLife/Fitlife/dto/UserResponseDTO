package fitlife.dto;

import fitlife.model.User.Role;

public class UserResponseDTO {
    private String name;
    private String email;
    private Role role;

    // Constructor de conveniencia
    public UserResponseDTO(String name, String email, Role role) {
        this.name = name;
        this.email = email;
        this.role = role;
    }

    // Getters y setters
}
